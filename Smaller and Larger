class Solve {
    int[] getMoreAndLess(int[] arr, int n, int x) {
        // code here
        int greater=0,smaller=0;
        int arr1[]=new int[2];
        for(int i=0;i<n;i++)
        {
           if(arr[i]>=x)
         greater++;
        
        if(arr[i]<=x)
         smaller++; 
        }
        arr1[0]=smaller;
        arr1[1]=greater;
        return arr1;
        
    }
}
