class Solution
{
    boolean check_elements(int arr[], int n, int A, int B)
    {
        ArrayList<Integer> al = new ArrayList<Integer>();
      
      for(int i=0;i<n;i++){
          al.add(arr[i]);
      }
      
      for(int i=A;i<=B;i++){
          if(al.contains(i)){
              continue;
          }
          else{
              return false;
          }
      }
      return true;


    }
}
