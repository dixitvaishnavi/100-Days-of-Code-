class Solution
{
    public int find_median(int[] v)
    {
        // Code here
        int mid=0;
        Arrays.sort(v);
        int size=v.length;
        if(size%2==0)
        {
           mid=(v[(size/2)-1]+v[(size/2)])/2;
        }
        else
        mid=v[((size+1)/2)-1];
        
        return mid;
    }
}
